        <servlet-name>outbox</servlet-name>
        <url-pattern>/outbox</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>conduit</servlet-name>
        <url-pattern>/conduit</url-pattern>
    </servlet-mapping>
