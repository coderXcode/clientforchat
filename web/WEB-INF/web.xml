        <servlet-class>mobo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>inbox</servlet-name>
        <servlet-class>inbox</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>outbox</servlet-name>
        <servlet-class>outbox</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>conduit</servlet-name>
        <servlet-class>conduit</servlet-class>
